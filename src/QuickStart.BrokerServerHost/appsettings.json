{
  "EQueue": {
    //本地IP地址
    "LocalAddress": "127.0.0.1",
    //NameServer地址和端口号,以','隔开
    "NameServerAddress": "127.0.0.1:9493",
    //Broker名称
    "BrokerName": "Broker1",
    //Broker分组名称
    "GroupName": "BrokerGroup1",
    //生产者端口
    "ProducerPort": 5000,
    //消费者端口
    "ConsumerPort": 5001,
    //管理端口
    "AdminPort": 5002,
    //文件存放目录
    "FileStoreRootPath": "d:\\equeue-store1",
    //是否为内存模式,如果内存模式则不会把消息存放在内存
    "IsMemoryMode": false,
    //是否同步刷盘
    "SyncFlush": false,
    //删除时是否忽略未消费过的消息
    "DeleteMessageIgnoreUnConsumed": false,
    //消息到达时是否立即通知Pull
    "NotifyWhenMessageArrived": true,
    //Chunk大小,以MB为单位
    "ChunkSize": 256,
    //Chunk写缓存,以KB为单位
    "ChunkWriteBuffer": 256,
    //是否开启缓存
    "EnableCache": false,
    //Chunk刷新时间间隔,以毫秒为单位
    "FlushInterval": 100,
    //Chunk最小缓存数量
    "ChunkCacheMinCount": 1,
    //Chunk最大缓存数量
    "ChunkCacheMaxCount": 2,
    //消息写入缓冲队列阀值
    "MessageWriteQueueThreshold": 10000
  }
}